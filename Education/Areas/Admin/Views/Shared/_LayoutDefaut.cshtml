<!DOCTYPE html>
<html lang="en">
<head>
    <!-- Required meta tags -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <link rel="shortcut icon" href="images/favicon.png" />
    <title>Edu - @ViewData["Title"]</title>

    <!-- Google Fonts -->
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,300;0,400;0,600;0,700;1,300;1,400;1,600;1,700&display=swap" rel="stylesheet">

    <!-- Lib CSS Files -->
    <link href="~/lib/feather/feather.css" rel="stylesheet" />
    <link href="~/lib/ti-icons/css/themify-icons.css" rel="stylesheet" />
    <link href="~/css/vendor.bundle.base.css" rel="stylesheet" />

    <!-- Template Main CSS File -->
    <link href="~/lib/datatables.net-bs4/datatables.bootstrap4.css" rel="stylesheet" />
    <link href="~/css/select.datatables.min.css" rel="stylesheet" />
    <link href="~/css/vertical-layout-light.css" rel="stylesheet" />
    <link href="~/css/style-admin.css" rel="stylesheet" />
</head>
<body>
    <!-- Container scroller -->
    <div class="container-scroller">
        <!-- Page body wrapper -->
        <div class="container-fluid page-body-wrapper full-page-wrapper">
            <!-- Content wrapper -->
            <div class="content-wrapper d-flex align-items-center auth px-0">
                <div class="row w-100 mx-0">
                    <div class="col-lg-4 mx-auto">
                        <!-- Main -->
                        @RenderBody()
                        <!-- Main ends -->
                    </div>
                </div>
            </div>
            <!-- Content wrapper ends -->
        </div>
        <!-- Page body wrapper ends -->
    </div>
    <!-- Container scroller ends -->

    <!-- Template Main JS File -->
    <script src="~/js/vendor.bundle.base.js"></script>
    <script src="~/lib/chart.js/chart.min.js"></script>
    <script src="~/js/jquery.datatables.js"></script>
    <script src="~/js/datatables.bootstrap4.js"></script>
    <script src="~/js/datatables.select.min.js"></script>
    <script src="~/js/off-canvas.js"></script>
    <script src="~/js/hoverable-collapse.js"></script>
    <script src="~/js/template.js"></script>
    <script src="~/js/todolist.js"></script>
    <script src="~/js/settings.js"></script>

    <!-- Template Main JS File -->
    <script src="~/js/dashboard.js"></script>
    <script src="~/js/chart.roundedbarcharts.js"></script>
</body>
</html>

